# station1.py
import math

def fib(n):
    if n == 0: return 0
    a, b = 0, 1
    for _ in range(2, n + 1):
        a, b = b, a + b
    return b

def solution_station_1(n: int):
    return math.factorial(n) if n >= 90 else fib(n)

if __name__ == "__main__":
    tests = [95, 98, 70, 71, 72, 73, 76, 88, 91, 67, 19, 22, 49]
    for x in tests:
        print(f"{x} -> {solution_station_1(x)}")

